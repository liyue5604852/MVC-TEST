<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liyue.www.dao.IUserDao">

	<select id="getPwdByNm" parameterType="java.lang.String" resultType="java.lang.String">
		select PASSWORD from USER_TEST
		where NAME = #{name}
	</select>
	
</mapper>